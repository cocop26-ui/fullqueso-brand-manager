      "parameters": {
        "jsCode": "// Extract Twilio webhook data\nconst body = $input.first().json.body;\n\nconst fromNumber = body.From || \"\";\nconst messageBody = body.Body || \"\";\nconst profileName = body.ProfileName || \"Cliente\";\nconst messageSid = body.MessageSid || \"\";\n\n// Clean phone number\nconst clientPhone = fromNumber.replace(\"whatsapp:\", \"\").replace(\"+\", \"\");\n\nif (!clientPhone || !messageBody) {\n  throw new Error(\"Missing phone or message body\");\n}\n\nreturn [{\n  json: {\n    clientPhone,\n    messageBody,\n    profileName,\n    messageSid,\n    fromNumber\n  }\n}];"
      },
